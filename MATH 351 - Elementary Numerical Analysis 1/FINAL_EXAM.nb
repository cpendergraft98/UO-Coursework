Cell[TextData[{
 StyleBox["Suppose that \[Phi](h) = L - 2h - ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{"4", 
    SuperscriptBox["h", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"cb74affd-a1f2-4ea7-a91a-11f6638153bc"],
 StyleBox[" - ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{"8", 
    SuperscriptBox["h", "3"]}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"2ad1c7a2-f5b6-4176-9bb1-afd942df3c19"],
 StyleBox[" + ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{"O", "(", 
    SuperscriptBox["h", "4"], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",
