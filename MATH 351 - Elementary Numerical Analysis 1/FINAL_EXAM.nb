Cell[TextData[{
 "\t",
 StyleBox["(a) For each of the cardinal polynomials ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptL]", "i"], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"0561dc82-f15d-413b-9e1a-38fe2a972f43"],
 StyleBox[" associated with the nodes, find ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox[
     UnderscriptBox["\[Integral]", 
      RowBox[{"-", "2"}]], "2"], 
    SubscriptBox["\[ScriptL]", "i"]}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"cfaf56da-4a66-47c1-bf9a-b7d42db6146b"],
 StyleBox["(x) dx.",
  FontWeight->"Bold"]
}], "Text",
 CellChangeTimes->{{3.816441414845961*^9, 3.8164414562984076`*^9}, {
  3.816441490070734*^9, 3.8164415005378003`*^9}, {3.8164415997124186`*^9, 
  3.8164416289719696`*^9}, {3.8164619129069595`*^9, 
  3.8164619133617463`*^9}},ExpressionUUID->"c697d268-7bb9-4b72-b907-\
20857b93ec1d"],

Cell[TextData[{
 "\t\tFirst, we recall that the Lagrange method tells us that the cardinal \
polynomials take the form ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["l", "i"], TraditionalForm]],ExpressionUUID->
  "9e38df4f-f920-4518-8ae6-e5a949a99f53"],
 " (x) =  ",
 Cell[BoxData[
  FormBox[
   UnderscriptBox[
    OverscriptBox["\[CapitalPi]", "n"], 
    RowBox[{
     RowBox[{"j", " ", "=", " ", "0"}], "\[IndentingNewLine]", 
     RowBox[{"j", "\[NotEqual]", " ", "i"}]}]], TraditionalForm]],
  ExpressionUUID->"64de56b8-25fb-4563-b4bc-cf78d365fe1d"],
 " ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"x", " ", "-", " ", 
     SubscriptBox["x", "j"]}], 
    RowBox[{
     SubscriptBox["x", "i"], " ", "-", " ", 
     SubscriptBox["x", "j"]}]], TraditionalForm]],ExpressionUUID->
  "ebe3673c-e50d-4bf5-83e6-021ae2350db5"],
 ". With this, we can construct our cardinal polynomials as follows:"
}], "Text",
 CellChangeTimes->{{3.81646192012267*^9, 
  3.8164620259407887`*^9}},ExpressionUUID->"1a27ec43-714c-4e24-8384-\
e86b55106f8a"],

Cell[BoxData[
 RowBox[{" ", 
  RowBox[{
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{"l0", ",", " ", "l1", ",", " ", "l2"}], "]"}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     SubscriptBox["x", "j"], " ", "=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "1"}], " ", "and", " ", 
       SubscriptBox["x", "i"]}], " ", "=", " ", 
      RowBox[{"0", " ", "and", " ", "2"}]}]}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"l0", "[", "x_", "]"}], " ", "=", " ", 
     RowBox[{"x", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"x", "-", "2"}], ")"}], "/", "3"}]}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     SubscriptBox["x", "j"], " ", "=", " ", 
     RowBox[{
      RowBox[{"0", " ", "and", " ", 
       SubscriptBox["x", "i"]}], " ", "=", " ", 
      RowBox[{
       RowBox[{"-", "1"}], " ", "and", " ", "2"}]}]}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"l1", "[", "x_", "]"}], " ", "=", " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", " ", "+", " ", "1"}], ")"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"x", " ", "-", " ", "2"}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"-", "2"}], ")"}]}]}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     SubscriptBox["x", "j"], " ", "=", " ", 
     RowBox[{
      RowBox[{"2", " ", "and", " ", 
       SubscriptBox["x", "i"]}], " ", "=", " ", 
      RowBox[{
       RowBox[{"-", "1"}], " ", "and", " ", "0"}]}]}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"l2", "[", "x_", "]"}], " ", "=", " ", 
     RowBox[{"x", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"x", " ", "+", " ", "1"}], ")"}], "/", "6"}]}]}], ";"}], 
   "\[IndentingNewLine]"}]}]], "Input",
 CellChangeTimes->{{3.8164620401178875`*^9, 3.8164622654405384`*^9}},
 CellLabel->
  "In[169]:=",ExpressionUUID->"19f30b5e-d6bf-481b-9179-6a305ac649ff"],

Cell[TextData[{
 "\t",
 StyleBox["(b) Use the values the you found in part (a) to derive an \
approximation formula of the form:\n\n\t\t",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox[
     UnderscriptBox["\[Integral]", 
      RowBox[{"-", "2"}]], "2"], 
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], " ", "dx"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"5d487df7-e8c3-4c74-83c3-d676100a226f"],
 StyleBox[" = ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"844eaf72-d9b2-4c60-9618-7489032e5337"],
 StyleBox[" * f(-1) + ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"e25288ce-9d0f-4d05-9812-eac9f1acbfd6"],
 StyleBox[" *  f(0) + ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"987644e7-62f5-4d3f-9c20-7ed45b3d20fd"],
 StyleBox[" * f(2)",
  FontWeight->"Bold"]
}], "Text",
 CellChangeTimes->{{3.816441652911066*^9, 3.816441801739518*^9}, {
  3.816462373863695*^9, 3.8164623748221292`*^9}},
 FontWeight->"Plain",ExpressionUUID->"91f830e1-0c06-4d12-9144-ab36357cd00a"],

Cell[TextData[{
 "\t\tTo begin, we recall that the Lagrange polynomial takes the form ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["p", "n"], "(", "x", ")"}], " ", "="}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "75368a7f-6ec9-47f6-9ca2-0d9bf33e1ed0"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox[
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"i", " ", "=", " ", "0"}]], "n"], 
    RowBox[{
     SubscriptBox["l", "i"], "(", "x", ")"}], 
    RowBox[{"f", "(", 
     SubscriptBox["x", "i"], ")"}]}], TraditionalForm]],ExpressionUUID->
  "6fe2aab3-6238-407d-a549-7565fc137ec2"],
 ", and we also have ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     SubscriptBox["x", "0"], ")"}], " ", "=", " ", 
    RowBox[{"f", "(", 
     RowBox[{"-", "1"}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "df25c33d-c9e9-4d4f-b64a-8a4fbcea0d1c"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     SubscriptBox["x", "1"], ")"}], " ", "=", " ", 
    RowBox[{"f", "(", "0", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d917712c-a5dc-479d-9e7e-a6d1daf5044a"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     SubscriptBox["x", "2"], ")"}], " ", "=", " ", 
    RowBox[{
     RowBox[{"f", "(", "2", ")"}], "."}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8fd3dc1f-e0b4-4678-bea1-12bcf606c26f"],
 " \n\t\tFurther, we should keep in mind that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["p", "n"], "(", "x", ")"}], " ", "\[TildeTilde]", " ", 
    RowBox[{"f", "(", "x", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3c3e5821-092a-4326-b29b-e1e69a726bcb"],
 ", and as such ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox[
      UnderscriptBox["\[Integral]", 
       RowBox[{"-", "2"}]], "2"], 
     RowBox[{
      RowBox[{"f", "(", "x", ")"}], "dx"}]}], " ", "=", " ", 
    RowBox[{
     UnderscriptBox[
      OverscriptBox["\[Integral]", "2"], 
      RowBox[{"-", "2"}]], 
     RowBox[{
      SubscriptBox["p", "n"], "(", "x", ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "20580d73-837e-4810-b25e-c56402d8dcfb"],
 ". \n\t\tThus, given that \n\t\t\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "n"], "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "dbd7866f-775a-4b22-858f-a2505a101ce4"],
 " = l0(x)*f(-1) + l1(x)*f(0) + l2*f(2) \n\t\t\n\t\twe can say that\n\t\t\n\t\
\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox[
      UnderscriptBox["\[Integral]", 
       RowBox[{"-", "2"}]], "2"], 
     RowBox[{
      RowBox[{"f", "(", "x", ")"}], "dx"}]}], " ", "=", " ", 
    RowBox[{
     UnderscriptBox[
      OverscriptBox["\[Integral]", "2"], 
      RowBox[{"-", "2"}]], 
     RowBox[{
      RowBox[{"[", 
       RowBox[{
        RowBox[{
         RowBox[{"l0", "(", "x", ")"}], "*", 
         RowBox[{"f", "(", 
          RowBox[{"-", "1"}], ")"}]}], " ", "+", " ", 
        RowBox[{
         RowBox[{"l1", "(", "x", ")"}], "*", 
         RowBox[{"f", "(", "0", ")"}]}], " ", "+", " ", 
        RowBox[{
         RowBox[{"l2", "(", "x", ")"}], "*", 
         RowBox[{"f", "(", "2", ")"}]}]}], "]"}], "dx"}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d69aa8fa-c116-4101-ab5b-1fc385847438"],
 ". \n\t\tSince all the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     SubscriptBox["x", "i"], ")"}], " "}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e8a182f1-6534-4bc2-b035-eb832ee856da"],
 " terms are considered to be constants that we know, this can be further re \
written as\n\t\t\n\t\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox[
      UnderscriptBox["\[Integral]", 
       RowBox[{"-", "2"}]], "2"], 
     RowBox[{
      RowBox[{"f", "(", "x", ")"}], "dx"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "(", 
       RowBox[{"-", "1"}], ")"}], "*", 
      RowBox[{
       UnderscriptBox[
        OverscriptBox["\[Integral]", "2"], 
        RowBox[{"-", "2"}]], 
       RowBox[{
        RowBox[{"l0", "(", "x", ")"}], "dx"}]}]}], "+", " ", 
     RowBox[{
      RowBox[{"f", "(", "0", ")"}], "*", 
      RowBox[{
       UnderscriptBox[
        OverscriptBox["\[Integral]", "2"], 
        RowBox[{"-", "2"}]], 
       RowBox[{
        RowBox[{"l1", "(", "x", ")"}], "dx"}]}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{"f", "(", "2", ")"}], "*", 
      RowBox[{
       UnderscriptBox[
        OverscriptBox["\[Integral]", "2"], 
        RowBox[{"-", "2"}]], 
       RowBox[{
        RowBox[{"l2", "(", "x", ")"}], "dx"}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"b96f640f-ca90-4921-b144-de27476ccea6"],
 ". \n\t\t \n\t\t If we further say that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["A", "0"], "=", " "}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f3070474-e7da-4f14-9b46-94fdcfa63179"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox[
     OverscriptBox["\[Integral]", "2"], 
     RowBox[{"-", "2"}]], 
    RowBox[{
     RowBox[{"l0", "(", "x", ")"}], "dx"}]}], TraditionalForm]],
  ExpressionUUID->"7d504312-c202-46ab-833c-5888398a89c6"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["A", "1"], " ", "=", 
     RowBox[{
      UnderscriptBox[
       OverscriptBox["\[Integral]", "2"], 
       RowBox[{"-", "2"}]], 
      RowBox[{
       RowBox[{"l1", "(", "x", ")"}], "dx"}]}]}], ",", " "}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c99efdc3-c6aa-49e8-a77d-4c5e695b4a0b"],
 "and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["A", "2"], " ", "=", " "}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a8a76004-ab20-4708-9c3b-2485dfef2dfb"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox[
     OverscriptBox["\[Integral]", "2"], 
     RowBox[{"-", "2"}]], 
    RowBox[{
     RowBox[{"l2", "(", "x", ")"}], "dx"}]}], TraditionalForm]],
  ExpressionUUID->"bba38e6f-ab64-4d88-a6b1-bf38b0fab7b3"],
 ", we see that we achieve an equation of the form:\n\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox[
     UnderscriptBox["\[Integral]", 
      RowBox[{"-", "2"}]], "2"], 
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], " ", "dx"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Plain",ExpressionUUID->"3c7e97ed-f889-4ec2-a685-2c0ebd62fc86"],
 " = ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Plain",ExpressionUUID->"82b0c487-41ec-4096-adba-02f595b98f78"],
 " * f(-1) + ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Plain",ExpressionUUID->"e888dc35-0ebf-4b27-ad9d-6cbb958080c9"],
 " *  f(0) + ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Plain",ExpressionUUID->"0b168d3b-e1ea-4d7c-9228-2667d07ca32b"],
 " * f(2)\n\t\t\n\t\tAll that is left to do now is compute our values for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["A", "0"], ",", " ", 
    SubscriptBox["A", "1"], ",", " ", 
    RowBox[{"and", " ", 
     RowBox[{
      SubscriptBox["A", "2"], "."}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6bad6e1c-69b0-46b0-a5f0-b496836cbc34"]
}], "Text",
 CellChangeTimes->{{3.8164622820501375`*^9, 3.8164622889925785`*^9}, {
  3.8164624225487423`*^9, 3.8164624992375956`*^9}, {3.816462545480976*^9, 
  3.8164628239592514`*^9}, {3.8164629600520124`*^9, 3.816463238650303*^9}, {
  3.8164633016380205`*^9, 
  3.8164634279143953`*^9}},ExpressionUUID->"3b452d81-3ee6-4a47-a7a2-\
6a73000b678f"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    SubscriptBox["A", "0"], " ", "="}], " ", "*)"}], " ", 
  RowBox[{
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"l0", "[", "x", "]"}], ",", " ", 
