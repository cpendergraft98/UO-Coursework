Cell["\<\
\t\tFirst off, 10/10 math pun, loved that.

\t\tOnce again, we have been provided with a function that we can execute for \
this problem.\
\>", "Text",
 CellChangeTimes->{{3.8164425587135057`*^9, 3.8164425685238686`*^9}, {
   3.816452913879616*^9, 3.8164529436310844`*^9}, 
   3.8164531586942463`*^9},ExpressionUUID->"a802a8da-ecc5-4169-9b25-\
d66c2758c39b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"SimpsonsRule", "[", 
   RowBox[{"f_", ",", 
    RowBox[{"{", 
     RowBox[{"x_", ",", "x0_", ",", "xn_"}], "}"}], ",", 
    RowBox[{"n_Integer:", "2"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"h", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"xn", "-", "x0"}], ")"}], "/", "n"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"Function", "[", 
         RowBox[{"x", ",", "f"}], "]"}], "[", "x0", "]"}], "+", 
       "\[IndentingNewLine]", 
       RowBox[{"4", 
        RowBox[{"Plus", "@@", 
         RowBox[{
          RowBox[{"Function", "[", 
           RowBox[{"x", ",", "f"}], "]"}], "/@", 
          RowBox[{"(", 
           RowBox[{"x0", "+", 
            RowBox[{"h", " ", 
             RowBox[{"Range", "[", 
              RowBox[{"1", ",", 
               RowBox[{"n", "-", "1"}], ",", "2"}], "]"}]}]}], ")"}]}]}]}], 
       "+", "\[IndentingNewLine]", 
       RowBox[{"2", 
        RowBox[{"Plus", "@@", 
         RowBox[{
          RowBox[{"Function", "[", 
           RowBox[{"x", ",", "f"}], "]"}], "/@", 
          RowBox[{"(", 
           RowBox[{"x0", "+", 
            RowBox[{"h", " ", 
             RowBox[{"Range", "[", 
              RowBox[{"2", ",", 
               RowBox[{"n", "-", "2"}], ",", "2"}], "]"}]}]}], ")"}]}]}]}], 
       "+", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Function", "[", 
         RowBox[{"x", ",", "f"}], "]"}], "[", "xn", "]"}]}], ")"}], 
     RowBox[{"h", "/", "3"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellLabel->"In[60]:=",ExpressionUUID->"ebeefc49-1609-4d6a-9e48-10f384519c76"],

Cell["\<\
In this case we will be using n_integer = 16, f = f(x), x0 = a = 0, xn = b = \
