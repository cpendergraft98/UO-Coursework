Cell[BoxData[
 RowBox[{
  RowBox[{"dividedDifference", "[", "t_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"m", ",", "xData", ",", "yData", ",", "n"}], "}"}], ",", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
       RowBox[{"We", "'"}], "ll", " ", "locally", " ", "hold", " ", "a", " ", 
