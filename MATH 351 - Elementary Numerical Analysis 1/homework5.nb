  RowBox[{"Simplify", "[", 
   RowBox[{"newtonForm", "[", "data1", "]"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"p", "[", "20", "]"}]}], "Input",
 CellChangeTimes->{{3.8135420346672287`*^9, 3.8135421307410955`*^9}, {
  3.813542167910524*^9, 3.8135422088271837`*^9}, {3.813542252293417*^9, 
  3.813542278747053*^9}, {3.813542375459143*^9, 3.8135423799989915`*^9}},
