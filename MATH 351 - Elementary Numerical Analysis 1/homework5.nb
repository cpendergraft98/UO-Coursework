Cell[BoxData[
 RowBox[{
  RowBox[{"newtonForm", "[", "t_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"coeffs", ",", "xData", ",", " ", "sum", ",", "prod", ",", "n"}],
