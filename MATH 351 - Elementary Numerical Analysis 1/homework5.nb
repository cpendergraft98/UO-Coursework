      RowBox[{
      "and", " ", "then", " ", "keep", " ", "a", " ", "table", " ", "of", " ",
        "coefficients"}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{"Length", "[", "t", "]"}]}], ";", 
     RowBox[{"xData", "=", 
      RowBox[{
       RowBox[{"Transpose", "[", "t", "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"coeffs", "=", 
      RowBox[{"Diagonal", "[", 
       RowBox[{"dividedDifference", "[", "t", "]"}], "]"}]}], ";", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<List of coefficients:\>\"", ",", "coeffs"}], "]"}], ";", 
     RowBox[{"sum", "=", 
      RowBox[{"coeffs", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "Initialize", " ", "with", " ", "the", " ", "constant", " ", "term"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"prod", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"term", "=", "1"}], ",", 
       RowBox[{"term", "<", "n"}], ",", 
       RowBox[{"term", "++"}], ",", 
       RowBox[{"(*", 
        RowBox[{"Remember", ",", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"we", "'"}], "re", " ", "building", " ", "a", " ", 
           "degree", " ", "n"}], "-", 
          RowBox[{"1", " ", "poly"}]}]}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"prod", "*=", 
         RowBox[{"(", 
          RowBox[{"x", "-", 
           RowBox[{"xData", "[", 
            RowBox[{"[", "term", "]"}], "]"}]}], ")"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"sum", "+=", 
         RowBox[{
          RowBox[{"coeffs", "[", 
           RowBox[{"[", 
            RowBox[{"term", "+", "1"}], "]"}], "]"}], "*", "prod"}]}]}]}], 
      " ", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "1", "st", " ", "coefficient", " ", "is", " ", "already", " ", 
         "there"}], ",", " ", 
        RowBox[{"so", " ", 
         RowBox[{"we", "'"}], "re", " ", "going", " ", "from", " ", "2", " ", 
         "to", " ", "n"}]}], "*)"}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "sum", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
