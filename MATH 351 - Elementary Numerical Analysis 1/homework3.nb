Cell[TextData[{
 "This method actually output a larger positive root than the previous \
method, specifying the root on the 4th iteration to be 1.83424.\n\nAnd last \
but not least, let us do these calculations using the secant method, the \
algorithm for which is below. Im going to select ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"0", " "}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e8c4478b-62dc-4764-9d0c-d89dfafea3e6"],
 " = 2 and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "719e3ca8-d1a0-4243-8648-eec16f0d841f"],
 " = 3:"
}], "Text",
 CellChangeTimes->{{3.8123227210292587`*^9, 3.812322722820669*^9}, {
  3.812322999476323*^9, 3.8123231075994773`*^9}, {3.8123231376883593`*^9, 
  3.812323172049389*^9}},ExpressionUUID->"ba15f39d-0a71-42ef-b374-\
3143b58d9548"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "x", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"secant", "[", 
    RowBox[{"f_", ",", "x0_", ",", "x1_", ",", "max_", ",", "delta_"}], "]"}],
    ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"k", "=", "0"}], ",", 
       RowBox[{"p0", "=", 
        RowBox[{"N", "[", "x0", "]"}]}], ",", 
       RowBox[{"p1", "=", 
        RowBox[{"N", "[", "x1", "]"}]}], ",", "m"}], "}"}], ",", " ", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"k", "=", "0"}], ",", 
        RowBox[{"k", "<", "max"}], ",", 
        RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{"f", "/.", 
              RowBox[{"x", "\[Rule]", "p0"}]}], "]"}], "<", 
            RowBox[{"Abs", "[", 
             RowBox[{"f", "/.", 
              RowBox[{"x", "\[Rule]", "p1"}]}], "]"}]}], ",", 
           RowBox[{"swap", "[", 
            RowBox[{"p0", ",", "p1"}], "]"}]}], "]"}], ";", " ", 
         RowBox[{"(*", 
          RowBox[{
          "We", " ", "always", " ", "want", " ", "p1", " ", "to", " ", "be", 
           " ", "\"\<better\>\""}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"m", "=", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"f", "/.", 
               RowBox[{"x", "\[Rule]", "p1"}]}], ")"}], "-", 
             RowBox[{"(", 
              RowBox[{"f", "/.", 
               RowBox[{"x", "\[Rule]", "p0"}]}], ")"}]}], ")"}], "/", 
           RowBox[{"(", 
            RowBox[{"p1", "-", "p0"}], ")"}]}]}], ";", 
         RowBox[{"(*", 
          RowBox[{"Slope", " ", "of", " ", "Secant"}], "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"m", "==", "0"}], ",", 
           RowBox[{
            RowBox[{"Print", "[", 
             RowBox[{
             "\"\<Found a horizontal secant at xi =\>\"", ",", " ", "p0", 
              ",", "\"\< xi+1 =\>\"", ",", "p1"}], "]"}], ";", 
            RowBox[{"Return", "[", "p1", "]"}]}]}], "]"}], ";", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{"Let", "'"}], "s", " ", "not", " ", "divide", " ", "by", 
           " ", "zero", " ", "in", " ", "the", " ", "next", " ", 
           RowBox[{"step", "!"}]}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"p0", "=", " ", "p1"}], ";", "\[IndentingNewLine]", 
         RowBox[{"p1", "=", 
          RowBox[{"p1", "-", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"f", "/.", 
              RowBox[{"x", "\[Rule]", "p1"}]}], ")"}], "/", "m"}]}]}], ";", 
         "\[IndentingNewLine]", " ", 
         RowBox[{"Print", "[", 
          RowBox[{"\"\<p = \>\"", ",", 
           RowBox[{"N", "[", "p1", "]"}], ",", "\"\< and y= \>\"", ",", 
           RowBox[{"N", "[", 
            RowBox[{"f", "/.", 
             RowBox[{"x", "\[Rule]", "p1"}]}], "]"}], ",", "\"\< dp = \>\"", 
           ",", 
           RowBox[{"N", "[", 
            RowBox[{"p1", "-", "p0"}], "]"}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{"p1", "-", "p0"}], "]"}], "<", "delta"}], ",", 
           RowBox[{
            RowBox[{"Print", "[", 
             RowBox[{
             "\"\<Accuracy attained since delta is\>\"", ",", " ", "delta", 
              ",", "\"\<and dp is:\>\"", ",", 
              RowBox[{"N", "[", 
               RowBox[{"p1", "-", "p0"}], "]"}]}], "]"}], ";", " ", 
            "\[IndentingNewLine]", 
            RowBox[{"Return", "[", "p1", "]"}]}]}], "]"}], ";"}]}], 
       RowBox[{"(*", 
        RowBox[{"Have", " ", "we", " ", "acheived", " ", 
         RowBox[{"accuracy", "?"}]}], "*)"}], "\[IndentingNewLine]", 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "p1", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.81232311343048*^9, 3.8123231167626934`*^9}},
 CellLabel->
  "In[174]:=",ExpressionUUID->"eb0d797f-ffc8-4eb5-a4ea-03f1c93ec715"],

