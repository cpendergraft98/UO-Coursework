Cell[BoxData[
 RowBox[{
  RowBox[{"Newton", "[", 
   RowBox[{"f_", ",", "p_", ",", "max_", ",", "delta_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"p0", ",", "p1", ",", "Y0", ",", "DY0", ",", "dp"}], "}"}], ",", 
    " ", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"input", " ", "function"}], ",", " ", 
      RowBox[{"first", " ", "guess"}], ",", " ", 
      RowBox[{"#", " ", "of", " ", "iterations"}], ",", " ", 
      RowBox[{"and", " ", "accuracy"}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"p0", "=", "p"}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Module", " ", 
        RowBox[{"vars", ":", " ", 
         RowBox[{"current", " ", "estimate"}]}]}], ",", " ", 
       RowBox[{"next", " ", "estimate"}], ",", " ", 
       RowBox[{"y", "-", "value"}], ",", " ", "slope", ",", " ", 
       RowBox[{"and", " ", "distance", " ", "\"\<improved\>\""}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "0"}], ",", 
       RowBox[{"i", "<", "max"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Y0", "=", 
         RowBox[{"N", "[", 
          RowBox[{"f", "/.", 
           RowBox[{"x", "\[Rule]", "p0"}]}], "]"}]}], ";", " ", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"Numerical", " ", "current", " ", "y"}], "-", "value"}], 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{"DY0", " ", "=", " ", 
         RowBox[{"N", "[", 
          RowBox[{
           RowBox[{"D", "[", 
            RowBox[{"f", ",", "x"}], "]"}], "/.", 
           RowBox[{"x", "\[Rule]", "p0"}]}], "]"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{"Numerical", " ", "current", " ", "slope"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"DY0", "==", "0"}], ",", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{
            "\"\<Found a horizontal tangent at x =\>\"", ",", " ", "p0"}], 
            "]"}], ";", 
           RowBox[{"Return", "[", "p0", "]"}]}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"p1", "=", " ", 
         RowBox[{"p0", "-", 
          RowBox[{"Y0", "/", "DY0"}]}]}], ";", 
        RowBox[{"(*", 
         RowBox[{"Compute", " ", "next", " ", "iteration"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"dp", "=", 
         RowBox[{"Abs", "[", 
          RowBox[{"p0", "-", "p1"}], "]"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{
         "Keep", " ", "track", " ", "of", " ", "how", " ", "much", " ", "we", 
          " ", "moved", " ", "this", " ", "step"}], "*)"}], 
        "\[IndentingNewLine]", " ", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\<p = \>\"", ",", 
          RowBox[{"N", "[", "p1", "]"}], ",", "   ", "\"\<\\t  |dp| <= \>\"", 
          ",", 
          RowBox[{"N", "[", "dp", "]"}], ",", "\"\<\\t iteration: \>\"", ",", 
          "i"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"dp", "<", "delta"}], ",", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{
            "\"\<Accuracy attained since delta is\>\"", ",", " ", "delta", 
             ",", "\"\< and dp is \>\"", ",", "dp"}], "]"}], ";", 
           RowBox[{"Return", "[", "p1", "]"}]}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"p0", "=", "p1"}], ";"}]}], " ", 
      RowBox[{"(*", 
       RowBox[{
       "Make", " ", "new", " ", "estimate", " ", "the", " ", "current", " ", 
        "estimate", " ", "and", " ", "repeat"}], "*)"}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "p0", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
