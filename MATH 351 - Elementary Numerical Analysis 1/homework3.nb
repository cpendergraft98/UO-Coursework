Cell[TextData[{
 "So for Newton\[CloseCurlyQuote]s method, it takes us 4 iterations through \
the program to reach an approximation for ",
 Cell[BoxData[
  FormBox[
   SqrtBox["2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "09b35e0a-7aec-4c77-aa29-bc3460b4b08c"],
 ", which is pretty quick.\n\nNow for the bisection method, we can use the \
algorithm from lab3 in order to automate the process and see how much more \
efficient or inefficient the bisection method is in comparison to the Newton \
method:"
}], "Text",
 CellChangeTimes->{{3.812318909098192*^9, 3.8123189690789104`*^9}, {
  3.8123190025998383`*^9, 3.812319049786695*^9}, {3.812319356140958*^9, 
  3.81231935980816*^9}},ExpressionUUID->"285b9682-e77f-4e4f-9caf-\
5cd373cb66be"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Bisection", "[", " ", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"a0_", ",", "b0_"}], "}"}], ",", " ", "Max_"}], "]"}], ":=", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
       RowBox[{"Assumes", " ", "a", " ", "pre"}], "-", 
       RowBox[{"defined", " ", "function", " ", "f"}]}], ",", " ", 
      RowBox[{
      "input", " ", "interval", " ", "and", " ", "number", " ", "of", " ", 
       "iterations"}]}], " ", "*)"}], "\n", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "a", ",", "b", ",", "c", ",", "Ya", ",", "Yb", ",", "Yc", ",", "k", 
        ",", "dx"}], "}"}], ",", "\n", "  ", 
      RowBox[{
       RowBox[{"a", " ", "=", " ", 
        RowBox[{"N", "[", "a0", "]"}]}], ";", " ", 
       RowBox[{"b", " ", "=", " ", 
        RowBox[{"N", "[", "b0", "]"}]}], ";", "\n", "  ", 
       RowBox[{"k", " ", "=", " ", "0"}], ";", " ", 
       RowBox[{"Ya", " ", "=", " ", 
        RowBox[{"f", "[", "a", "]"}]}], ";", " ", 
       RowBox[{"Yb", " ", "=", " ", 
        RowBox[{"f", "[", "b", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Ya", "*", "Yb"}], ">", "0"}], ",", 
         RowBox[{
          RowBox[{"Print", "[", " ", 
           RowBox[{
           "Ya", ",", "\"\< and \>\"", ",", " ", "Yb", ",", " ", 
            "\"\< have the same sign, so no guaranteed root.\>\""}], "]"}], 
          ";", " ", 
          RowBox[{"Return", "[", "]"}]}]}], "]"}], ";", " ", 
       RowBox[{"(*", 
        RowBox[{
        "Make", " ", "sure", " ", "the", " ", "user", " ", "has", " ", 
         "defined", " ", "an", " ", "appropriate", " ", "interval"}], " ", 
        "*)"}], "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"Ya", " ", "==", "0"}], ",", 
         RowBox[{
          RowBox[{"Print", "[", " ", 
           RowBox[{"a", ",", " ", "\"\< is an exact root!\>\""}], "]"}], ";", 
          " ", 
          RowBox[{"Return", "[", "a", "]"}]}]}], "]"}], ";", " ", 
       RowBox[{"(*", 
        RowBox[{
        "Maybe", " ", "the", " ", "user", " ", "imputs", " ", "a", " ", 
         "root"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"Yb", " ", "==", "0"}], ",", 
         RowBox[{
          RowBox[{"Print", "[", " ", 
           RowBox[{"b", ",", " ", "\"\< is an exact root!\>\""}], "]"}], ";", 
          " ", 
          RowBox[{"Return", "[", "b", "]"}]}]}], "]"}], ";", "\n", "  ", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"k", "=", "0"}], ",", " ", 
         RowBox[{"k", " ", "<", " ", "Max"}], ",", " ", 
         RowBox[{"k", "++"}], ",", " ", 
         RowBox[{"(*", 
          RowBox[{
          "Run", " ", "this", " ", "for", " ", "loop", " ", "Max", " ", 
           "times"}], " ", "*)"}], "\n", "    ", 
         RowBox[{
          RowBox[{"c", "  ", "=", " ", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], ";", "\n", "    ", 
          RowBox[{"Yc", " ", "=", " ", 
           RowBox[{"f", "[", "c", "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"Yc", " ", "==", "0"}], ",", 
            RowBox[{
             RowBox[{"Print", "[", " ", 
              RowBox[{"c", ",", " ", "\"\< is an exact root!\>\""}], "]"}], 
             ";", " ", 
             RowBox[{"Return", "[", "c", "]"}]}]}], "]"}], ";", "\n", "    ", 
          RowBox[{"dx", " ", "=", " ", 
           RowBox[{"b", "-", "a"}]}], ";", "\n", "    ", 
          RowBox[{"Print", "[", 
           RowBox[{"\"\<k = \>\"", ",", "k", ",", "\"\<\\t    c = \>\"", ",", 
            RowBox[{"N", "[", "c", "]"}], ",", "\"\<\\t  error <= \>\"", ",", 
            RowBox[{"N", "[", "dx", "]"}]}], "]"}], ";", "\n", "      ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Yb", "*", "Yc"}], ">", "0"}], ",", " ", 
            RowBox[{
             RowBox[{"b", "=", "c"}], ";", "  ", 
             RowBox[{"Yb", "=", "Yc"}]}], ",", 
            RowBox[{"(*", " ", 
             RowBox[{
              RowBox[{"If", " ", "the", " ", "function", " ", 
               RowBox[{"doesn", "'"}], "t", " ", "change", " ", "signs", " ", 
               "in", " ", 
               RowBox[{"(", 
                RowBox[{"b", ",", "c"}], ")"}]}], ",", " ", 
              RowBox[{"we", " ", "use", " ", "the", " ", "interval", " ", 
               RowBox[{"(", 
                RowBox[{"a", ",", "c"}], ")"}]}]}], " ", "*)"}], 
            "\[IndentingNewLine]", " ", 
            RowBox[{
             RowBox[{"a", "=", "c"}], ";", "   ", 
             RowBox[{"Ya", "=", "Yc"}]}]}], "]"}], ";"}]}], " ", "]"}], ";", 
       " ", 
       RowBox[{"(*", " ", 
        RowBox[{"Otherwise", " ", "we", " ", "use", " ", 
         RowBox[{"(", 
          RowBox[{"b", ",", "c"}], ")"}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "c", "]"}]}]}], "]"}]}], ";"}], 
  "    "}]], "Input",
 CellChangeTimes->{{3.812319096713326*^9, 3.812319098401663*^9}},
 CellLabel->
  "In[132]:=",ExpressionUUID->"77974c5c-f13e-471c-b8bd-4dbad20d186f"],

