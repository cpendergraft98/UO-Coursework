Cell[TextData[{
 "Establish Newton\[CloseCurlyQuote]s iterative scheme in simplified form, \
not involving the reciprocal of x, for the function f(x) = xR - ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", 
    RowBox[{"-", "1"}]], TraditionalForm]],ExpressionUUID->
  "ec7ce267-def6-4a28-ae31-0955e91eb0e3"],
 ". Carry out three steps of this procedure using R = 4 and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "0"], TraditionalForm]],ExpressionUUID->
  "131f8752-d3d1-4f5e-9d78-8512567fb3b0"],
 " = -1.\n\nWe can once again use Newton\[CloseCurlyQuote]s method for \
continuous and differentiable functions here:\n\n",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"n", "+", "1", " "}]], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontSize->24,ExpressionUUID->"2abcc67e-b595-4c56-8d58-f8ed13f9de42"],
 StyleBox["= ",
  FontSize->24],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontSize->24,ExpressionUUID->"9cf2442d-0445-41e3-977c-798079d6b990"],
 StyleBox[" - ",
  FontSize->24],
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"f", "(", 
     SubscriptBox["x", "n"], ")"}], 
    RowBox[{
     RowBox[{"f", "'"}], 
     RowBox[{"(", 
      SubscriptBox["x", "n"], ")"}]}]], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontSize->24,ExpressionUUID->"33df39d2-a7dc-4b7f-9a95-6d3b1d7e9727"],
 "\n\nSubbing in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", 
    SubscriptBox["x", "n"], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a4ee03c0-6d95-4f1b-9028-2f9d1046369f"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "'"}], 
    RowBox[{"(", 
     SubscriptBox["x", "n"], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8cf951c5-6bfc-41c2-8427-befae893f1b5"],
 " we get;\n\n",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"n", "+", "1", " "}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "61503624-ea2a-4f92-b87a-f5127719673e"],
 StyleBox["= ",
  FontSize->24],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "26639bd3-383c-444c-b647-e262683cb66f"],
 StyleBox[" - ",
  FontSize->24],
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SubscriptBox["x", "n"], "R"}], " ", "-", " ", 
      SuperscriptBox[
       SubscriptBox["x", "n"], 
       RowBox[{"-", "1"}]]}], ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "/", 
        SuperscriptBox[
         SubscriptBox["x", "n"], "2"]}], ")"}], " ", "+", " ", "R"}], ")"}]], 
   TraditionalForm]],
  FormatType->"TraditionalForm",
  FontSize->24,ExpressionUUID->"de9ac61c-ccb7-419f-8ddd-fa02cdf8b904"],
 "\n\nNow we have a nice little iterative sequence we can use to approximate \
further values "
}], "Text",
 CellChangeTimes->{{3.812294188863507*^9, 3.812294297401258*^9}, {
  3.8122945624982004`*^9, 3.8122945875063934`*^9}, {3.812308999324155*^9, 
  3.8123090683386126`*^9}, {3.8123093012562504`*^9, 3.812309303546965*^9}, {
  3.8123100585587015`*^9, 3.812310776272376*^9}, {3.812310862105399*^9, 
  3.8123113376215963`*^9}, {3.812311415868212*^9, 3.8123114185414944`*^9}, {
  3.812313793424876*^9, 3.812313924997332*^9}, {3.8123139559402437`*^9, 
  3.812314035298396*^9}, {3.8123140696693892`*^9, 3.812314101951917*^9}, {
  3.812314141039373*^9, 3.812314142219784*^9}, {3.812314313170667*^9, 
  3.812314315099843*^9}, {3.8123144589731317`*^9, 
  3.8123144955966105`*^9}},ExpressionUUID->"d763e07e-3b5c-4722-9972-\
50b36bf575fc"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "f", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], " ", "=", " ", 
   RowBox[{"x", " ", "-", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"4", "x"}], " ", "-", " ", 
        RowBox[{"x", "^", 
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}]}]}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "/", 
          RowBox[{"x", "^", "2"}]}], ")"}], "+", "4"}], ")"}]}], ")"}]}]}], 
