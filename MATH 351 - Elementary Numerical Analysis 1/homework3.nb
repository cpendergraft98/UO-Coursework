   RowBox[{
    RowBox[{"x", "^", "3"}], " ", "-", " ", 
    RowBox[{"5", "x"}], " ", "+", " ", "3"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Bisection", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"1", ",", " ", "3"}], "}"}], ",", " ", "5"}], "]"}]}], "Input",
