   RowBox[{"1", "/", "2"}], ")"}], 
  RowBox[{"(", 
   RowBox[{"2", " ", "-", " ", "0"}], ")"}], 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"f", "[", "2", "]"}], " ", "+", " ", 
    RowBox[{"f", "[", "0", "]"}]}], ")"}]}]}], "Input",
 CellChangeTimes->{{3.8160448585577593`*^9, 3.8160449105741158`*^9}, {
