Cell[BoxData[
 RowBox[{
  RowBox[{"SimpsonsRule", "[", 
   RowBox[{"f_", ",", 
    RowBox[{"{", 
     RowBox[{"x_", ",", "x0_", ",", "xn_"}], "}"}], ",", 
    RowBox[{"n_Integer:", "2"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"h", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"xn", "-", "x0"}], ")"}], "/", "n"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"Function", "[", 
         RowBox[{"x", ",", "f"}], "]"}], "[", "x0", "]"}], "+", 
       "\[IndentingNewLine]", 
       RowBox[{"4", 
        RowBox[{"Plus", "@@", 
         RowBox[{
          RowBox[{"Function", "[", 
           RowBox[{"x", ",", "f"}], "]"}], "/@", 
          RowBox[{"(", 
           RowBox[{"x0", "+", 
            RowBox[{"h", " ", 
             RowBox[{"Range", "[", 
              RowBox[{"1", ",", 
               RowBox[{"n", "-", "1"}], ",", "2"}], "]"}]}]}], ")"}]}]}]}], 
       "+", "\[IndentingNewLine]", 
       RowBox[{"2", 
        RowBox[{"Plus", "@@", 
         RowBox[{
          RowBox[{"Function", "[", 
           RowBox[{"x", ",", "f"}], "]"}], "/@", 
          RowBox[{"(", 
           RowBox[{"x0", "+", 
            RowBox[{"h", " ", 
             RowBox[{"Range", "[", 
              RowBox[{"2", ",", 
               RowBox[{"n", "-", "2"}], ",", "2"}], "]"}]}]}], ")"}]}]}]}], 
       "+", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Function", "[", 
         RowBox[{"x", ",", "f"}], "]"}], "[", "xn", "]"}]}], ")"}], 
     RowBox[{"h", "/", "3"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellLabel->
  "In[520]:=",ExpressionUUID->"87f311c3-983f-4707-8e55-13f6554b6193"],

Cell[CellGroupData[{

Cell["5.3 # 4", "Section",
 CellChangeTimes->{{3.8160346181596327`*^9, 
