Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Bisection", "[", " ", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"a0_", ",", "b0_"}], "}"}], ",", " ", "Max_"}], "]"}], ":=", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
       RowBox[{"Assumes", " ", "a", " ", "pre"}], "-", 
       RowBox[{"defined", " ", "function", " ", "f"}]}], ",", " ", 
      RowBox[{
      "input", " ", "interval", " ", "and", " ", "number", " ", "of", " ", 
       "iterations"}]}], " ", "*)"}], "\n", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "a", ",", "b", ",", "c", ",", "Ya", ",", "Yb", ",", "Yc", ",", "k", 
        ",", "dx"}], "}"}], ",", "\n", "  ", 
      RowBox[{
       RowBox[{"a", " ", "=", " ", 
        RowBox[{"N", "[", "a0", "]"}]}], ";", " ", 
       RowBox[{"b", " ", "=", " ", 
        RowBox[{"N", "[", "b0", "]"}]}], ";", "\n", "  ", 
       RowBox[{"k", " ", "=", " ", "0"}], ";", " ", 
       RowBox[{"Ya", " ", "=", " ", 
        RowBox[{"f", "[", "a", "]"}]}], ";", " ", 
       RowBox[{"Yb", " ", "=", " ", 
        RowBox[{"f", "[", "b", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Ya", "*", "Yb"}], ">", "0"}], ",", 
         RowBox[{
          RowBox[{"Print", "[", " ", 
           RowBox[{
           "Ya", ",", "\"\< and \>\"", ",", " ", "Yb", ",", " ", 
            "\"\< have the same sign, so no guaranteed root.\>\""}], "]"}], 
          ";", " ", 
          RowBox[{"Return", "[", "]"}]}]}], "]"}], ";", " ", 
       RowBox[{"(*", 
        RowBox[{
        "Make", " ", "sure", " ", "the", " ", "user", " ", "has", " ", 
         "defined", " ", "an", " ", "appropriate", " ", "interval"}], " ", 
        "*)"}], "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"Ya", " ", "==", "0"}], ",", 
         RowBox[{
          RowBox[{"Print", "[", " ", 
           RowBox[{"a", ",", " ", "\"\< is an exact root!\>\""}], "]"}], ";", 
          " ", 
          RowBox[{"Return", "[", "a", "]"}]}]}], "]"}], ";", " ", 
       RowBox[{"(*", 
        RowBox[{
        "Maybe", " ", "the", " ", "user", " ", "imputs", " ", "a", " ", 
         "root"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"Yb", " ", "==", "0"}], ",", 
         RowBox[{
          RowBox[{"Print", "[", " ", 
           RowBox[{"b", ",", " ", "\"\< is an exact root!\>\""}], "]"}], ";", 
          " ", 
          RowBox[{"Return", "[", "b", "]"}]}]}], "]"}], ";", "\n", "  ", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"k", "=", "0"}], ",", " ", 
         RowBox[{"k", " ", "<", " ", "Max"}], ",", " ", 
         RowBox[{"k", "++"}], ",", " ", 
         RowBox[{"(*", 
          RowBox[{
          "Run", " ", "this", " ", "for", " ", "loop", " ", "Max", " ", 
           "times"}], " ", "*)"}], "\n", "    ", 
         RowBox[{
          RowBox[{"c", "  ", "=", " ", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], ";", "\n", "    ", 
          
          RowBox[{"Yc", " ", "=", " ", 
           RowBox[{"f", "[", "c", "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"Yc", " ", "==", "0"}], ",", 
            RowBox[{
             RowBox[{"Print", "[", " ", 
              RowBox[{"c", ",", " ", "\"\< is an exact root!\>\""}], "]"}], 
             ";", " ", 
             RowBox[{"Return", "[", "c", "]"}]}]}], "]"}], ";", "\n", "    ", 
          
          RowBox[{"dx", " ", "=", " ", 
           RowBox[{"b", "-", "a"}]}], ";", "\n", "    ", 
          RowBox[{"Print", "[", 
           RowBox[{
           "\"\<k = \>\"", ",", "k", ",", "\"\<\\t    c = \>\"", ",", 
            RowBox[{"N", "[", "c", "]"}], ",", "\"\<\\t  error <= \>\"", ",", 
            
            RowBox[{"N", "[", "dx", "]"}]}], "]"}], ";", "\n", "      ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Yb", "*", "Yc"}], ">", "0"}], ",", " ", 
            RowBox[{
             RowBox[{"b", "=", "c"}], ";", "  ", 
             RowBox[{"Yb", "=", "Yc"}]}], ",", 
            RowBox[{"(*", " ", 
             RowBox[{
              RowBox[{"If", " ", "the", " ", "function", " ", 
               RowBox[{"doesn", "'"}], "t", " ", "change", " ", "signs", " ", 
               "in", " ", 
               RowBox[{"(", 
                RowBox[{"b", ",", "c"}], ")"}]}], ",", " ", 
              RowBox[{"we", " ", "use", " ", "the", " ", "interval", " ", 
               RowBox[{"(", 
                RowBox[{"a", ",", "c"}], ")"}]}]}], " ", "*)"}], 
            "\[IndentingNewLine]", " ", 
            RowBox[{
             RowBox[{"a", "=", "c"}], ";", "   ", 
             RowBox[{"Ya", "=", "Yc"}]}]}], "]"}], ";"}]}], " ", "]"}], ";", 
       " ", 
       RowBox[{"(*", " ", 
        RowBox[{"Otherwise", " ", "we", " ", "use", " ", 
         RowBox[{"(", 
          RowBox[{"b", ",", "c"}], ")"}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "c", "]"}]}]}], "]"}]}], ";"}], 
  "    "}]], "Input",
 CellLabel->"In[1]:=",ExpressionUUID->"3b1640b0-ac77-4227-9a3d-3a60c5e4441c"],

