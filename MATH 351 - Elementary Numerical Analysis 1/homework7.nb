Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"trapezoidRule", "[", 
    RowBox[{"f_", ",", "a_", ",", "b_", ",", "n_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"h", ",", "est"}], "}"}], ",", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
        RowBox[{"current", " ", "width", " ", "of", " ", "a", " ", "sub"}], 
        "-", "interval"}], ",", " ", 
       RowBox[{"and", " ", "the", " ", "current", " ", "estimate"}]}], "*)"}],
      "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"h", "=", 
       RowBox[{"b", "-", "a"}]}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"We", "'"}], "ll", " ", "do", " ", "the", " ", "base", " ", 
        "case", " ", "ourselves"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"est", "=", 
       RowBox[{".5", "*", "h", "*", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"f", "/.", 
            RowBox[{"x", "\[Rule]", "b"}]}], ")"}], "+", 
          RowBox[{"(", 
           RowBox[{"f", "/.", 
            RowBox[{"x", "\[Rule]", "a"}]}], ")"}]}], ")"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{
       "\"\<Initial estimate with one subinterval: \>\"", ",", "est"}], "]"}],
       ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"k", "=", "1"}], ",", 
        RowBox[{"k", "\[LessEqual]", "n"}], ",", 
        RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"h", "=", 
          RowBox[{"h", "/", "2"}]}], ";", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{"Doubling", " ", "number", " ", "of", " ", "intervals"}], 
           ",", " ", 
           RowBox[{"halving", " ", "the", " ", "width"}]}], "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"est", "=", 
          RowBox[{
           RowBox[{"est", "/", "2"}], "+", 
           RowBox[{"h", "*", 
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{"f", "/.", 
               RowBox[{"x", "\[Rule]", 
                RowBox[{"(", 
                 RowBox[{"a", "+", 
                  RowBox[{
                   RowBox[{"(", 
                    RowBox[{
                    RowBox[{"2", "i"}], "-", "1"}], ")"}], "*", "h"}]}], 
                 ")"}]}]}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "1", ",", 
                RowBox[{"2", "^", 
                 RowBox[{"(", 
                  RowBox[{"k", "-", "1"}], ")"}]}]}], "}"}]}], "]"}]}]}]}], 
         ";", " ", 
         RowBox[{"(*", 
          RowBox[{
          "This", " ", "is", " ", "just", " ", "theorem", " ", "2", " ", 
           "from", " ", "the", " ", "section"}], "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"Print", "[", 
          RowBox[{
           RowBox[{"2", "^", "k"}], ",", "\"\< sub-intervals yields: \>\"", 
           ",", "est"}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "Iterate", " ", "through", " ", "the", " ", "powers", " ", "of", " ", 
        "2"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "est", "]"}]}]}], "]"}]}], 
  "\[IndentingNewLine]"}]], "Input",
 CellLabel->"In[27]:=",ExpressionUUID->"540aed4a-44d1-4d34-a671-1e110e5c9935"],

